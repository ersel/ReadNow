{
  "name": "ReadNow: Open Bookmarks From Queue",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Opens bookmarks added by ReadLater app in a new tab.",
  "icons": {
    "16": "images/icon-16.png",
    "96": "images/icon-96.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "permissions": [
    "storage",
    "bookmarks",
    "history",
    "tabs",
    "webNavigation"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "browser_action": {
    "default_icon": {
      "19": "images/icon-19.png",
      "38": "images/icon-38.png"
    },
    "default_title": "ReadNow",
    "default_popup": "popup.html"
  }
}